{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "semi": [2, "never"],
    "no-unreachable": 2,
    "no-unexpected-multiline": 2,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "globals": {
    "CustomElements": false,
    "HTMLImports": false,
    "Polymer": false,
    "WeakMap": false
  }
}
